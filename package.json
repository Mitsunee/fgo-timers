{
  "name": "fgo-tools",
  "version": "0.1.0",
  "private": true,
  "packageMananger": "yarn",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "next dev",
    "dev:data": "node src/scripts/watch/watch.mjs",
    "upgrades:legacy": "node src/scripts/upgrades-legacy/update-upgrades-data.mjs",
    "build": "node --version && next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test:scripts": "uvu tests/scripts",
    "test:data": "node src/scripts/check/check.mjs",
    "test": "yarn run test:scripts && yarn run test:data --all"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{js,jsx,mjs}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md,yml}": [
      "prettier -w"
    ],
    "**/!(*inkscape).svg": [
      "svgo"
    ],
    "assets/data/{events,login-tickets}/*.yml": [
      "node src/scripts/check/check.mjs --silent --file"
    ],
    "assets/data/{mana,rare}PrismShop.yml": [
      "node src/scripts/check/check.mjs --silent --file"
    ]
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not ie 11"
  ],
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@foxkit/node-util": "^0.2.1",
    "@nanostores/persistent": "^0.5.2",
    "@nanostores/react": "0.1.5",
    "@svgo/jsx": "^0.3.0",
    "classcat": "^5.0.3",
    "foxkit": "^0.3.0",
    "ioredis": "^5.0.3",
    "modern-diacritics": "^2.2.1",
    "modern-normalize": "^1.1.0",
    "msgpackr": "^1.5.5",
    "nanostores": "^0.5.12",
    "next": "12.1.4",
    "node-fetch": "^3.2.3",
    "picoapi": "^0.1.2",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.12",
    "postcss-focus": "^5.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.3",
    "postcss-theme-ui": "^0.10.0",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "spacetime": "^7.1.0",
    "svgo": "^2.7.0",
    "typescript": "4.5.5",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "commander": "^9.1.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "8.12.0",
    "eslint-config-foxkit": "^1.0.1",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "nano-staged": "^0.6.0",
    "simple-git-hooks": "^2.6.1",
    "uvu": "^0.5.3"
  },
  "engines": {
    "node": "14.x"
  }
}
