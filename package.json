{
  "name": "fgo-tools",
  "version": "0.1.0",
  "private": true,
  "packageMananger": "yarn",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "next dev",
    "dev:data": "node src/scripts/watch/watch.mjs",
    "upgrades:legacy": "node src/scripts/upgrades-legacy/update-upgrades-data.mjs",
    "prebuild": "node --version && node src/scripts/prebuild/prebuild.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test:server": "jest --testMatch **/tests/server/**/*.test.js",
    "test:scripts": "uvu tests/scripts",
    "test:data": "node src/scripts/check/check.mjs",
    "test": "yarn run test:server && yarn run test:scripts && yarn run test:data --all"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{js,jsx,mjs}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md,yml}": [
      "prettier -w"
    ],
    "**/!(*inkscape).svg": [
      "svgo"
    ],
    "assets/data/{events,login-tickets}/*.yml": [
      "node src/scripts/check/check.mjs --silent --file"
    ],
    "assets/data/{mana,rare}PrismShop.yml": [
      "node src/scripts/check/check.mjs --silent --file"
    ]
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not ie 11"
  ],
  "dependencies": {
    "@babel/core": "^7.19.0",
    "@foxkit/node-util": "^0.4.0",
    "@nanostores/persistent": "^0.7.0",
    "@nanostores/react": "0.4.0",
    "@svgo/jsx": "^0.3.0",
    "classcat": "^5.0.3",
    "foxkit": "^0.3.0",
    "modern-diacritics": "^2.2.1",
    "modern-normalize": "^1.1.0",
    "nanostores": "^0.7.0",
    "next": "12.3.0",
    "node-fetch": "^3.2.10",
    "picoapi": "^0.2.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-focus": "^5.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.8.1",
    "postcss-theme-ui": "^0.10.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "spacetime": "^7.2.0",
    "svgo": "^2.7.0",
    "typescript": "4.8.3",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "13.4.0",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "8.23.1",
    "eslint-config-foxkit": "^1.1.1",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.8.0",
    "uvu": "^0.5.4"
  },
  "engines": {
    "node": "16.x"
  }
}
