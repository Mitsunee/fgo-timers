{
  "name": "fgo-tools",
  "version": "0.1.0",
  "private": true,
  "packageMananger": "yarn",
  "scripts": {
    "prepare": "simple-git-hooks && node src/svgo/build.mjs",
    "build:svgo": "node src/svgo/build.mjs --force",
    "check-event": "tsm --require=./nowarn.js src/events/check-event-script.ts",
    "find": "tsm --require=./nowarn.js src/utils/find-script.ts",
    "prebuild": "tsm --require=./nowarn.js src/prebuild/prebuild.ts && yarn build:svgo",
    "build": "node --version && next build",
    "dev": "yarn prebuild && next dev",
    "dev:types": "tsc --watch",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test:types": "tsc --noEmit",
    "test:code": "jest --verbose",
    "test:data": "tsm --require=./nowarn.js src/schema/check.ts",
    "test": "yarn test:types && yarn prebuild && yarn test:code && yarn test:data --all"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{js,jsx,cjs,mjs,ts,tsx,css,json,md,yml}": [
      "prettier -w"
    ],
    "**/!(*inkscape).svg": [
      "svgo"
    ]
  },
  "browserslist": [
    "last 3 versions",
    ">0.2% and not dead"
  ],
  "dependencies": {
    "@atlasacademy/api-connector": "^4.0.4",
    "@atlasacademy/api-descriptor": "^2.0.6",
    "@babel/core": "^7.20.12",
    "@csstools/postcss-global-data": "^1.0.2",
    "@foxkit/node-util": "^0.4.0",
    "@foxkit/util": "^0.6.1",
    "@nanostores/persistent": "^0.7.0",
    "@nanostores/react": "0.4.1",
    "@next/font": "^13.1.6",
    "@svgo/jsx": "^0.4.2",
    "@tanstack/react-query": "^4.24.6",
    "@trpc/client": "^10.11.1",
    "@trpc/next": "^10.11.1",
    "@trpc/react-query": "^10.11.1",
    "@trpc/server": "^10.11.1",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "classcat": "^5.0.3",
    "commander": "^10.0.0",
    "fast-fuzzy": "^1.12.0",
    "flag-icons": "^6.6.6",
    "modern-normalize": "^1.1.0",
    "nanostores": "^0.7.3",
    "next": "13.1.6",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-focus": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "spacetime": "^7.4.1",
    "svgo": "^3.0.2",
    "tsm": "^2.3.0",
    "typescript": "4.9.5",
    "yaml": "^2.2.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "8.34.0",
    "eslint-config-foxkit": "^2.0.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.8.1"
  },
  "engines": {
    "node": "16.x"
  }
}
