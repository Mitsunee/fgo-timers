{
  "name": "fgo-tools",
  "version": "0.1.0",
  "private": true,
  "packageMananger": "yarn",
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "next dev",
    "dev:data": "node src/scripts/watch/watch.mjs",
    "build": "node --version && next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test:scripts": "uvu tests/scripts",
    "test:data": "node src/scripts/check/check.mjs",
    "test": "yarn run test:scripts && yarn run test:data --all"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{js,jsx,mjs}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md,yml}": [
      "prettier -w"
    ],
    "**/!(*inkscape).svg": [
      "svgo"
    ],
    "assets/data/{events,login-tickets}/*.yml": [
      "node src/scripts/check/check.mjs --silent --file"
    ],
    "assets/data/{mana,rare}PrismShop.yml": [
      "node src/scripts/check/check.mjs --silent --file"
    ]
  },
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not ie 11"
  ],
  "dependencies": {
    "ioredis": "^4.28.5",
    "msgpackr": "^1.5.5",
    "next": "12.1.0",
    "picoapi": "^0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@foxkit/eslint-config": "1.0.2",
    "@foxkit/node-util": "^0.2.0",
    "@nanostores/persistent": "^0.5.0",
    "@nanostores/react": "^0.1.5",
    "@svgo/jsx": "^0.3.0",
    "chokidar": "^3.5.3",
    "classcat": "^5.0.3",
    "commander": "^9.0.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "7.32.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "foxkit": "^0.3.0",
    "modern-diacritics": "^2.0.0",
    "modern-normalize": "^1.1.0",
    "nano-staged": "^0.6.0",
    "nanostores": "^0.5.8",
    "node-fetch": "^3.0.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.5",
    "postcss-focus": "^5.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.0.2",
    "postcss-theme-ui": "^0.10.0",
    "prettier": "^2.5.1",
    "simple-git-hooks": "^2.6.1",
    "spacetime": "^7.1.0",
    "svgo": "^2.7.0",
    "typescript": "^4.5.4",
    "uvu": "^0.5.3",
    "yaml": "^1.10.2"
  },
  "engines": {
    "node": "14.x"
  }
}
