{
  "name": "fgo-tools",
  "version": "0.1.0",
  "private": true,
  "packageMananger": "yarn",
  "scripts": {
    "prepare": "simple-git-hooks && node src/svgo/build.mjs",
    "build:svgo": "node src/svgo/build.mjs --force",
    "dev:scripts": "tsc --watch -p tsconfig_scripts.json",
    "build:scripts": "tsc -p tsconfig_scripts.json",
    "prebuild": "tsm --require=./nowarn.js src/prebuild/prebuild.ts && yarn build:svgo",
    "build": "node --version && next build",
    "dev": "yarn prebuild && next dev",
    "dev:types": "tsc --watch",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test:types": "tsc --noEmit",
    "test:code": "jest --verbose",
    "test:data": "tsm --require=./nowarn.js src/schema/check.ts",
    "test": "yarn test:types && yarn prebuild && yarn test:code && yarn test:data --all"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn nano-staged"
  },
  "nano-staged": {
    "**/*.{js,jsx,mjs,ts,tsx}": [
      "eslint",
      "prettier -w"
    ],
    "**/*.{css,json,md,yml}": [
      "prettier -w"
    ],
    "**/!(*inkscape).svg": [
      "svgo"
    ],
    "assets/data/{events,login-tickets,items}/*.yml": [
      "tsm --require=./nowarn.js src/schema/check.ts --silent --file"
    ],
    "assets/data/{mana,rare}PrismShop.yml": [
      "tsm --require=./nowarn.js src/schema/check.ts --silent --file"
    ]
  },
  "browserslist": [
    "last 3 versions",
    ">0.2% and not dead"
  ],
  "dependencies": {
    "@atlasacademy/api-connector": "^3.0.124",
    "@atlasacademy/api-descriptor": "^1.0.137",
    "@babel/core": "^7.20.7",
    "@foxkit/node-util": "^0.4.0",
    "@foxkit/util": "^0.6.1",
    "@nanostores/persistent": "^0.7.0",
    "@nanostores/react": "0.4.1",
    "@svgo/jsx": "^0.3.0",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "classcat": "^5.0.3",
    "commander": "^9.4.1",
    "fast-fuzzy": "^1.12.0",
    "flag-icons": "^6.6.6",
    "modern-normalize": "^1.1.0",
    "nanostores": "^0.7.1",
    "next": "12.3.1",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.20",
    "postcss-focus": "^5.0.1",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "spacetime": "^7.3.0",
    "svgo": "^3.0.2",
    "swr": "^1.3.0",
    "tsm": "^2.3.0",
    "typescript": "4.9.4",
    "yaml": "^2.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint": "8.30.0",
    "eslint-config-foxkit": "^2.0.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.8.1"
  },
  "engines": {
    "node": "16.x"
  }
}
